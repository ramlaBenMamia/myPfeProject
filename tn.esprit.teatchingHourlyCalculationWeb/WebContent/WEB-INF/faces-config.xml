 <navigation-rule>
 	<display-name>pages/LoginEnseignant.xhtml</display-name>
 	<from-view-id>/pages/LoginEnseignant.xhtml</from-view-id>
 	<navigation-case>
 		<from-outcome>ok</from-outcome>
 		<to-view-id>/pages/AcceuilEnseignant.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 <navigation-rule>
 	<display-name>pages/LoginAdmin.xhtml</display-name>
 	<from-view-id>/pages/LoginAdmin.xhtml</from-view-id>
 	<navigation-case>
 		<from-outcome>ok</from-outcome>
 		<to-view-id>/pages/AcceuilAdministrateur.xhtml</to-view-id>
 	</navigation-case>
 </navigation-rule>
 
</faces-config>
